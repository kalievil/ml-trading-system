{
    "max_open_trades": 1,
    "stake_currency": "USDT",
    "stake_amount": "unlimited",
    "tradable_balance_ratio": 1.0,
    "fiat_display_currency": "USD",
    
    "dry_run_wallet": 10000,
    "cancel_open_orders_on_exit": false,
    
    "trading_mode": "spot",
    "margin_mode": "isolated",
    
    "stoploss": -0.018,
    "trailing_stop": false,
    "use_custom_stoploss": true,
    
    "minimal_roi": {
        "0": 0.015,
        "15": 0.010,
        "30": 0.005
    },
    
    "timeframe": "5m",
    "startup_candle_count": 200,
    "freqai": {
        "enabled": false,
        "train_period_days": 120,
        "purge_previous_results": true,
        "save_backtest_models": true,
        "data_split_parameters": {
            "test_size": 0.2,
            "random_state": 42,
            "train_period_days": 60,
            "test_period_days": 30,
            "step_period_days": 7
        },
        "feature_parameters": {
            "indicator_periods_candles": [14, 21],
            "include_timeframes": ["5m"],
            "include_corr_pairlist": [],
            "include_shifted_candles": 0,
            "label_period_candles": 2
        },
        "model_training_parameters": {
            "fit_live_predictions": false
        },
        "model": {
            "type": "XGBoostClassifier",
            "parameters": {
                "n_estimators": 150,
                "max_depth": 7,
                "learning_rate": 0.15,
                "subsample": 0.85,
                "colsample_bytree": 0.85,
                "random_state": 42
            }
        }
    },
    
    "exit_pricing": {
        "price_side": "same",
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        },
        "use_order_book": false,
        "order_book_top": 1,
        "ignore_roi_if_entry_signal": false
    },
    
    "entry_pricing": {
        "price_side": "same",
        "check_depth_of_market": {
            "enabled": false,
            "bids_to_ask_delta": 1
        },
        "use_order_book": false,
        "order_book_top": 1,
        "price_last_balance": 0.0,
        "check_depth_of_market_enabled": false,
        "bid_strategy": {
            "ask_last_balance": 0.0,
            "use_order_book": false,
            "order_book_top": 1,
            "check_depth_of_market_enabled": false,
            "price_side": "ask",
            "check_depth_of_market": {
                "enabled": false,
                "bids_to_ask_delta": 1
            }
        },
        "ask_strategy": {
            "bid_last_balance": 0.0,
            "use_order_book": false,
            "order_book_top": 1,
            "check_depth_of_market_enabled": false,
            "price_side": "bid",
            "check_depth_of_market": {
                "enabled": false,
                "bids_to_ask_delta": 1
            }
        }
    },
    
    "exchange": {
        "name": "binance",
        "key": "",
        "secret": "",
        "ccxt_config": {
            "enableRateLimit": true,
            "options": {
                "defaultType": "future"
            }
        },
        "ccxt_async_config": {
            "enableRateLimit": true,
            "options": {
                "defaultType": "future"
            }
        },
        "pair_whitelist": [
            "BTC/USDT"
        ],
        "pair_blacklist": []
    },
    
    
    
    "pairlists": [
        {
            "method": "StaticPairList"
        }
    ],

    "webserver": {
        "enabled": true,
        "listen_ip_address": "127.0.0.1",
        "listen_port": 8080
    },
    "api_server": {
        "enabled": true,
        "listen_ip_address": "127.0.0.1",
        "listen_port": 8080,
        "cors_origins": ["http://127.0.0.1:8080", "http://localhost:8080"],
        "username": "admin",
        "password": "admin",
        "jwt_secret_key": "change-this-secret",
        "log_requests": false
    }
}


