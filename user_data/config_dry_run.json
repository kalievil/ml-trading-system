{
  "$schema": "https://schema.freqtrade.io/schema.json",
  "max_open_trades": 3,
  "stake_currency": "USDT",
  "stake_amount": 100,
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",
  "timeframe": "5m",
  "dry_run": true,
  "cancel_open_orders_on_exit": true,
  "unfilledtimeout": {
    "entry": 10,
    "exit": 10,
    "exit_timeout_count": 0,
    "unit": "minutes"
  },
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1,
    "price_last_balance": 0.0,
    "check_depth_of_market": {
      "enabled": false,
      "bids_to_ask_delta": 1
    }
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": true,
    "order_book_top": 1
  },
  "exchange": {
    "name": "binance",
    "key": "dry_run_key",
    "secret": "dry_run_secret",
    "ccxt_config": {},
    "ccxt_async_config": {},
        "pair_whitelist": [
            "BTC/USDT:USDT"
        ],
    "pair_blacklist": []
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
    "telegram": {
        "enabled": false,
        "token": "dummy_token",
        "chat_id": "dummy_chat_id"
    },
  "api_server": {
    "enabled": true,
    "listen_ip_address": "127.0.0.1",
    "listen_port": 8080,
    "verbosity": "info",
    "jwt_secret_key": "your_jwt_secret_key_change_this",
    "CORS_origins": [
      "http://localhost:8080",
      "http://127.0.0.1:8080"
    ],
    "username": "freqtrader",
    "password": "SuperSecurePassword123"
  },
  "bot_name": "OptimizedXGBoostBot",
  "initial_state": "running",
  "force_entry_enable": false,
  "internals": {
    "process_throttle_secs": 5
  },
  "trading_mode": "futures",
  "margin_mode": "isolated",
  "liquidation_buffer": 0.05
}